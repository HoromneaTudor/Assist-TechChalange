{
  "expo": {
    "name": "eHotelPlus",
    "slug": "eHotelPlus",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./apps/assets/icon.png",
    "splash": {
      "image": "./apps/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
